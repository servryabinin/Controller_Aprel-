<Project Sdk="Microsoft.NET.Sdk.Web">

	<PropertyGroup>
		<TargetFramework>net8.0</TargetFramework>
		<Nullable>enable</Nullable>
		<ImplicitUsings>enable</ImplicitUsings>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="Newtonsoft.Json" Version="13.0.3" />
		<PackageReference Include="Swashbuckle.AspNetCore" Version="6.6.2" />
	</ItemGroup>

	<ItemGroup>
		<Content Include="Csvs\**\*.*" CopyToPublishDirectory="Always" />
		<Content Include="Jsons\**\*.*" CopyToPublishDirectory="Always" />
		<Content Include="Archive\**\*.*" CopyToPublishDirectory="Always" />
		<Content Include="certs\https.pfx" CopyToPublishDirectory="Always" />
	</ItemGroup>

</Project>
